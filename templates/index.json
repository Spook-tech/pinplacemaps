    "product_info_hAC8Uc": {
      "type": "product-info",
      "blocks": {
        "slider_image_HRENLj": {
          "type": "slider_image",
          "settings": {
            "image": "shopify://shop_images/DST_0319_copy.jpg"
          }
        },
        "slider_image_eNnmGP": {
          "type": "slider_image",
          "settings": {
            "image": "shopify://shop_images/DST_0369_copy_91f995e3-7ace-4993-84f4-6444f39e204f.jpg"
